	<!-- BEGIN calendar_page -->
	<form method="POST" action="{action_url}">
	<table align="center" width="60%" cellspacing="2" style="border: 1px solid #000000;">
		<tr class="th">
			<td colspan="2">&nbsp;<b>{lang_Calendar_settings}</b></td>
		</tr>
		<tr class="row_off">
			<td>{lang_Calendar_default_for_current_installation}:</td>
			<td>
				<select name="newsettings[defaultCalendar]"> 
					<option value="{selected}">{selected}</option> 
					<option value="{option}"  >{option}</option> 
				</select> 
			</td>
		</tr>   

		<tr>
			<td colspan="2" align="center">
			  <input type="submit" name="submit" value="{lang_save}">
			  <input type="submit" name="submit" value="{lang_cancel}">
			  <br>
			</td>
		</tr>
	</table>
	</form>
	<!-- END calendar_page -->
